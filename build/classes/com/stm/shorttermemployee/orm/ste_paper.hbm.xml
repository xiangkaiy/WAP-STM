<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.Paper" table="ste_paper">
		<id name="id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">ste_sequence</param>
			</generator>
		</id>
		<property name="question" column="question" type="string" />
		<property name="options" column="options" type="string" />
		<property name="answers" column="answers" type="string" />
		<property name="name" column="name" type="string" />
	</class>
</hibernate-mapping>