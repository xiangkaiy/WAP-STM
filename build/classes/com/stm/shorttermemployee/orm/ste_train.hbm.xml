<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.Train" table="ste_train">
		<id name="id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">ste_sequence</param>
			</generator>
		</id>
		<property name="hrid" column="hrid" type="long" />
		<property name="name" column="name" type="string" />
		<property name="starttime" column="starttime" type="timestamp" />
		<property name="endtime" column="endtime" type="timestamp" />
		<property name="paperid" column="paperid" type="long" />
		<property name="departid" column="departid" type="long" />
		<property name="required" column="required" type="string" />
	</class>
</hibernate-mapping>