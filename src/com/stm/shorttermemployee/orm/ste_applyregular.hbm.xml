<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.ApplyRegular" table="ste_applyregular">
		<id name="id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">ste_sequence</param>
			</generator>
		</id>
		<property name="steid" column="steid" type="long" />
		<property name="applytime" column="applytime" type="timestamp" />
		<property name="status" column="status" type="string" />
		<property name="leaderid" column="leaderid" type="long" />
		<property name="bossid" column="bossid" type="long" />
		<property name="hrid" column="hrid" type="long" />
		<property name="intro" column="intro" type="string" />
	</class>
</hibernate-mapping>