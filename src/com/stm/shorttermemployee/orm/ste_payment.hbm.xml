<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.Payment" table="ste_payment">
		<id name="id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">ste_sequence</param>
			</generator>
		</id>
		<property name="hrid" column="hrid" type="long" />
		<property name="givetime" column="givetime" type="timestamp" />
		<property name="startdate" column="startdate" type="date" />
		<property name="enddate" column="enddate" type="date" />
		<property name="money" column="money" type="float" />
		<property name="totaltime" column="totaltime" type="long" />
		<property name="steid" column="steid" type="long" />
		<property name="fine" column="fine" type="string" />
	</class>
</hibernate-mapping>