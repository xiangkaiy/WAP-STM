<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.SteLeave" table="ste_steleave">
		<id name="id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">ste_sequence</param>
			</generator>
		</id>
		<property name="steid" column="steid" type="long" />
		<property name="starttime" column="starttime" type="timestamp" />
		<property name="endtime" column="endtime" type="timestamp" />
		<property name="reason" column="reason" type="string" />
		<property name="applytime" column="applytime" type="timestamp" />
		<property name="status" column="status" type="string" />
		<property name="leadercomment" column="leadercomment" type="string" />
		<property name="leaderid" column="leaderid" type="long" />
	</class>
</hibernate-mapping>