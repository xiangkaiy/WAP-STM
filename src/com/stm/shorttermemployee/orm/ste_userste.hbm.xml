<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.UserSTE" table="ste_userste">
		<id name="id" column="id" type="long" />
		<property name="leasthoursweek" column="leasthoursweek" type="long"/>
		<property name="joindate" column="joindate" type="date"/>
		<property name="expectedsalary" column="expectedsalary" type="string"/>
		<property name="status" column="status" type="integer"/>
		<property name="officeskill" column="officeskill" type="string"/>
		<property name="programlanguage" column="programlanguage" type="string"/>
		<property name="currentsalary" column="currentsalary" type="float"/>
	</class>
</hibernate-mapping>