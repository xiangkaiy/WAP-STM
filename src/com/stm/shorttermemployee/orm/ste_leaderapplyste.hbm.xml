<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.LeaderApplySTE" table="ste_leaderapplyste">
		<id name="id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">ste_sequence</param>
			</generator>
		</id>
		<property name="leaderid" column="leaderid" type="long" />
		<property name="steamount" column="steamount" type="long" />
		<property name="officeskill" column="officeskill" type="string" />
		<property name="programlanguage" column="programlanguage"
			type="string" />
		<property name="applytime" column="applytime" type="timestamp" />
		<property name="expectjoindate" column="expectjoindate" type="date" />
		<property name="expectworkhour" column="expectworkhour" type="integer" />
		<property name="status" column="status" type="integer" />
	</class>
</hibernate-mapping>