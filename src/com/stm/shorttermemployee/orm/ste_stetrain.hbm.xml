<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.SteTrain" table="ste_stetrain">
		<id name="id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">ste_sequence</param>
			</generator>
		</id>
		<property name="steid" column="steid" type="long" />
		<property name="trainid" column="trainid" type="long" />
		<property name="score" column="score" type="long" />
		<property name="finishtime" column="finishtime" type="timestamp" />
		<property name="status" column="status" type="string" />
		<property name="answer" column="answer" type="string" />
	</class>
</hibernate-mapping>