<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stm.shorttermemployee.pojo.STEWorktime" table="ste_steworktime">
		<id name="id" column="id" type="long">
			<generator class="sequence">
				<param name="sequence">ste_sequence</param>
			</generator>
		</id>
		<property name="signtime" column="signtime" type="date" />
		<property name="starttime" column="starttime" type="timestamp" />
		<property name="endtime" column="endtime" type="timestamp" />
		<property name="totaltime" column="totaltime" type="long" />
		<property name="steid" column="steid" type="long" />
	</class>
</hibernate-mapping>